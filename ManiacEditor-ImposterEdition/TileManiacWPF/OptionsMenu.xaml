<Window x:Class="TileManiacWPF.OptionsMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:maniac="clr-namespace:TileManiacWPF"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Tile Maniac Options" Height="569" Width="1138" Background="{DynamicResource NormalBackground}" ResizeMode="NoResize" Closing="OptionBox_FormClosing" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="156*"/>
            <ColumnDefinition Width="127*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <TabControl x:Name="MainTabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.ColumnSpan="2" Margin="0,0,0,0" BorderBrush="{DynamicResource DisabledText}" Background="{DynamicResource NormalBackground}">
            <TabItem Header="Defaults" Foreground="{DynamicResource NormalText}">
                <Grid Background="{DynamicResource NormalBackground}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="415*"/>
                        <ColumnDefinition Width="711*"/>
                    </Grid.ColumnDefinitions>
                    <GroupBox Header="Common Defaults" Width="250" Height="120" VerticalAlignment="Top" HorizontalAlignment="Left">
                        <StackPanel>
                            <CheckBox Content="Scrolling Allowed in Classic Mode" Margin="2"  IsChecked="{maniac:SettingBinding ScrollAllowed}"></CheckBox>
                            <CheckBox Content="Show Grid" Margin="2"  IsChecked="{maniac:SettingBinding ShowGrid}"></CheckBox>
                            <CheckBox Content="Enable Classic Mode" Margin="2"  IsChecked="{maniac:SettingBinding ClassicMode}"></CheckBox>
                            <CheckBox Content="Enable Mirror Mode" Margin="2"  IsChecked="{maniac:SettingBinding MirrorMode}"></CheckBox>
                            <CheckBox Content="Window Always On Top" Margin="2"  IsChecked="{maniac:SettingBinding WindowAlwaysOnTop}"></CheckBox>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Default List View" Width="250" Height="60" Margin="4,120,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                        <StackPanel>
                            <RadioButton x:Name="collisionListRadioButton" Content="Collision" Margin="2" Click="ListViewModeChanged"></RadioButton>
                            <RadioButton x:Name="tileListRadioButton" Content="Tiles" Margin="2" Click="ListViewModeChanged"></RadioButton>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Tile Viewer Default View" Width="250" Height="80" Margin="4,180,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                        <StackPanel>
                            <RadioButton x:Name="collisionRenderViewRadioButton" Content="Collision" Margin="2" Click="RenderViewModeChanged"></RadioButton>
                            <RadioButton x:Name="tileRenderViewRadioButton" Content="Tile" Margin="2" Click="RenderViewModeChanged"></RadioButton>
                            <RadioButton x:Name="overlayRenderViewRadioButton" Content="Hybrid Overlay (Collision + Tiles)" Margin="2" Click="RenderViewModeChanged"></RadioButton>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Editor View Setting*" Width="250" Height="100" Margin="4,260,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                        <StackPanel>
                            <RadioButton x:Name="collisionEditorViewRadioButton" Content="Collision" Margin="2" Click="EditorViewModeChanged"></RadioButton>
                            <RadioButton x:Name="overlayEditorViewRadioButton" Content="Hybrid Overlay (Collision + Tiles)" Margin="2"  Click="EditorViewModeChanged"></RadioButton>
                            <TextBlock></TextBlock>
                            <TextBlock>*Not for Classic Mode</TextBlock>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Misc Settings" Width="250" Height="100" Margin="4,360,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                        <StackPanel>
                            <CheckBox x:Name="enableWindowsClipboard" Content="Enable Windows Clipboard*" Margin="2" IsChecked="{maniac:SettingBinding EnableWindowsClipboard}"></CheckBox>
                            <CheckBox Content="Use Dark Theme" Margin="2" x:Name="DarkModeCheckBox"></CheckBox>
                            <TextBlock></TextBlock>
                            <TextBlock>*Not Working ATM</TextBlock>
                        </StackPanel>
                    </GroupBox>
                    <StackPanel Grid.Column="1" Margin="0,0,10,10" Width="186" Height="115" HorizontalAlignment="Right" VerticalAlignment="Bottom">
                        <Button IsEnabled="False" Content="Import Settings" Height="20" Width="170" Margin="0,4"></Button>
                        <Button IsEnabled="False" Content="Export Settings"  Height="20" Width="170" Margin="0,4"></Button>
                        <Button Content="Reset Settings to Default" Click="ResetSettingsToDefault"  Height="20" Width="170" Margin="0,4"></Button>
                        <Button Content="Reset Controls to Default" Click="ResetControlsToDefault"  Height="20" Width="170" Margin="0,4"></Button>
                    </StackPanel>
                </Grid>
