<Window x:Class="ManiacEditor.Interfaces.OptionsMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManiacEditor.Interfaces"
        xmlns:maniac="clr-namespace:ManiacEditor"
        xmlns:p="clr-namespace:ManiacEditor.Properties"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Maniac Editor Options" Height="569" Width="1138" Background="{DynamicResource NormalBackground}" ResizeMode="NoResize" Closing="OptionBox_FormClosing" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="156*"/>
            <ColumnDefinition Width="127*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <TabControl x:Name="MainTabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.ColumnSpan="2" Margin="0,0,0,0" BorderBrush="{DynamicResource DisabledText}" Background="{DynamicResource NormalBackground}">

            <TabItem Header="Common" Foreground="{DynamicResource NormalText}">
                <Grid Background="{DynamicResource NormalBackground}">


                    <GroupBox Header="Various Settings" HorizontalAlignment="Left" Height="179" VerticalAlignment="Top" Width="509" Margin="10,10,0,0">
                        <StackPanel>
                            <CheckBox Content="Enable Copy between Scenes" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding ForceCopyUnlock}"/>
                            <CheckBox Content="Keep Main Layers on when Editing Extra Layers" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding KeepLayersVisible}"/>
                            <CheckBox Content="Enable Auto-Backup" HorizontalAlignment="Left" IsEnabled="False" VerticalAlignment="Top" Margin="10,2,0,0"/>
                            <CheckBox Content="Enable Asset Expansion Packs" IsEnabled="False" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0"/>
                            <CheckBox Content="Use File Explorer instead of Scene Select for Browsing Stages." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding forceBrowse}"/>
                            <CheckBox Content="Disable Save Warnings" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding DisableSaveWarnings}"/>
                            <CheckBox x:Name="DarkModeCheckBox" Content="Use Dark Theme (Requires Manual Restart)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0"/>
                            <CheckBox Content="Pre-Render TURBO Mode (Highly Experimental)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding preRenderTURBOMode}"/>
                            <CheckBox Content="Scoller Auto Centers with the Middle of the Scene Viewer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,2,0,0" IsChecked="{maniac:SettingBinding ScrollerAutoCenters}"/>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Game Utilites" Height="105" VerticalAlignment="Top" Margin="10,194,0,0" HorizontalAlignment="Left" Width="222">
                        <StackPanel>
                            <Label Content="Game Lock On Options" HorizontalContentAlignment="Center"></Label>
                            <Button Content="Settings..." Margin="5" Click="Button_Click_1"></Button>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Program Paths" HorizontalAlignment="Left" Height="180" VerticalAlignment="Top" Width="282" Margin="237,194,0,0">
                        <StackPanel>
                            <StackPanel>
                                <Label Content="RSDK Animation Editor Path" HorizontalContentAlignment="Center"></Label>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBox Width="130" HorizontalAlignment="Center" Text="{maniac:SettingBinding RunAniEdPath}"></TextBox>
                                    <Button Width="20" Content="..." Click="button15_Click"></Button>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel>
                                <Label Content="Color Palette Editor Path" HorizontalContentAlignment="Center"></Label>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBox Width="130" HorizontalAlignment="Center" Text="Removed" IsEnabled="False"></TextBox>
                                    <Button Width="20" Content="..." Click="button16_Click" IsEnabled="False"></Button>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel>
                                <Label Content="Tile Maniac Path" HorizontalContentAlignment="Center"></Label>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <TextBox Width="130" HorizontalAlignment="Center" Text="Removed" IsEnabled="False"></TextBox>
                                    <Button Width="20" Content="..." Click="button17_Click" IsEnabled="False"></Button>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Performance Defaults" HorizontalAlignment="Left" Height="178" VerticalAlignment="Top" Width="309" Margin="524,10,0,0">
                        <StackPanel>
                            <RadioButton x:Name="minimalRadioButton2" Content="Minimal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,4,0,0" Click="SetGraphicalPresetSetting" Tag="Minimal"/>
                            <RadioButton x:Name="basicRadioButton2" Content="Basic" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,4,0,0" Click="SetGraphicalPresetSetting" Tag="Basic"/>
                            <RadioButton x:Name="superRadioButton2" Content="Super" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,4,0,0" Click="SetGraphicalPresetSetting" Tag="Super"/>
                            <RadioButton x:Name="hyperRadioButton2" Content="Hyper" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,4,0,0" Click="SetGraphicalPresetSetting" Tag="Hyper"/>
                            <RadioButton x:Name="customRadioButton2" Content="Custom" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,4,0,0" Click="SetGraphicalPresetSetting" Tag="Custom" IsEnabled="False"/>
                            <Label HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">
                                <TextBlock Text="* Changing other settings outside of these settings will result in a custom configuration(this will not update immediately)" TextAlignment="Center" TextWrapping="WrapWithOverflow"/>
                            </Label>
                        </StackPanel>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="Defaults" Foreground="{DynamicResource NormalText}">
                <Grid Background="{DynamicResource NormalBackground}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="415*"/>
                        <ColumnDefinition Width="711*"/>
                    </Grid.ColumnDefinitions>
                    <GroupBox Header="Tile Toolbar Defaults" HorizontalAlignment="Left" Height="104" VerticalAlignment="Top" Width="193" Margin="10,10,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="62*"/>
                                <ColumnDefinition Width="29*"/>
                            </Grid.ColumnDefinitions>
                            <CheckBox Content="Solid (Top)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,4,0,0" IsChecked="{maniac:SettingBinding SolidTopDefault}"/>
                            <CheckBox Content="Solid (All excl. top)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,24,0,0" IsChecked="{maniac:SettingBinding SolidAllButTopDefault}"/>
                            <CheckBox Content="Solid (Top)  (Plane 2)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,44,0,0" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding Unkown1Default}"/>
                            <CheckBox Content="Solid (All excl. top)  (Plane 2)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,64,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding Unkown2Default}"/>
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="Custom FG Layers" HorizontalAlignment="Left" Height="131" Margin="10,122,0,0" VerticalAlignment="Top" Width="193">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="121*"/>
                                <ColumnDefinition Width="60*"/>
                            </Grid.ColumnDefinitions>
                            <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{maniac:SettingBinding CustomLayerLow}" VerticalAlignment="Top" Width="161" Margin="10,26,0,0" Grid.ColumnSpan="2"/>
                            <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{maniac:SettingBinding CustomLayerHigh}" VerticalAlignment="Top" Width="161" Margin="10,72,0,0" Grid.ColumnSpan="2"/>
                            <Label Content="Lower Layer:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0"/>
                            <Label Content="Higher Layer:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,46,0,0"/>
                        </Grid>

                    </GroupBox>
                    <GroupBox Header="Tile Toolbar Default Zoom Level" HorizontalAlignment="Left" Height="59" Margin="10,258,0,0" VerticalAlignment="Top" Width="193">
                        <Grid>
                            <Slider HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Height="30" Width="181" Ticks="0 1 2 3" TickPlacement="Both" Maximum="3" Value="{maniac:SettingBinding tileToolbarDefaultZoomLevel}"/>
                        </Grid>

                    </GroupBox>
                    <GroupBox Header="Scroll Lock Default" HorizontalAlignment="Left" Height="59" Margin="10,322,0,0" VerticalAlignment="Top" Width="193">
                        <Grid>
                            <RadioButton Checked="radioButtonX_CheckedChanged_1" Unchecked="radioButtonX_CheckedChanged_1" x:Name="radioButtonX" Content="X" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="40,0,0,0"/>
                            <RadioButton Checked="radioButtonY_CheckedChanged_1" Unchecked="radioButtonY_CheckedChanged_1" x:Name="radioButtonY" Content="Y" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="-40,0,0,0"/>

                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Scene Select View Default" HorizontalAlignment="Left" Margin="10,386,0,0" Width="193" Height="67" VerticalAlignment="Top">
                        <StackPanel>
                            <RadioButton x:Name="SceneSelectRadio2" Checked="radioButton2_CheckedChanged" Unchecked="radioButton2_CheckedChanged" Content="Files View" Margin="0,5,0,0" IsChecked="{maniac:SettingBinding SceneSelectRadioButton2On}"/>
                            <RadioButton x:Name="SceneSelectRadio1" Checked="radioButton1_CheckedChanged" Unchecked="radioButton1_CheckedChanged" Content="GameConfig View" Margin="0,5,0,0" IsChecked="{maniac:SettingBinding SceneSelectRadioButton1On}"/>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Entities Toolbar Visibility Defaults" HorizontalAlignment="Left" Height="104" VerticalAlignment="Top" Width="193" Margin="208,10,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="62*"/>
                            </Grid.ColumnDefinitions>
                            <CheckBox Content="Mania" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,4,0,0" IsChecked="{maniac:SettingBinding showManiaEntities}"/>
                            <CheckBox Content="Encore" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,24,0,0" IsChecked="{maniac:SettingBinding showEncoreEntities}"/>
                            <CheckBox Content="Both" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,44,0,0" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding showBothEntities}"/>
                            <CheckBox Content="Other" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,64,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding showOtherEntities}"/>

                            <CheckBox HorizontalAlignment="Left" VerticalAlignment="Top" VerticalContentAlignment="Center" Margin="68,39,0,0" Height="41" Width="84" IsChecked="{maniac:SettingBinding useBitOperators}">
                                <TextBlock Text="Use Bit Operators" TextWrapping="Wrap"/>
                            </CheckBox>
                            <CheckBox Content="Pinball" HorizontalAlignment="Left" Margin="68,4,0,62" IsChecked="{maniac:SettingBinding showPinballEntities}"/>
                            <CheckBox Content="Filterless" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="68,24,0,0" IsChecked="{maniac:SettingBinding showPrePlusEntities}"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Default Visible Layers" HorizontalAlignment="Left" Height="203" VerticalAlignment="Top" Width="193" Margin="208,114,0,0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="62*"/>
                                <ColumnDefinition Width="29*"/>
                            </Grid.ColumnDefinitions>
                            <CheckBox Content="FG Lower" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,4,0,0" IsChecked="{maniac:SettingBinding FGLowerDefault}"/>
                            <CheckBox Content="FG Low" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,24,0,0" IsChecked="{maniac:SettingBinding FGLowDefault}"/>
                            <CheckBox Content="FG High" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,44,0,0" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding FGHighDefault}"/>
                            <CheckBox Content="FG Higher" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,64,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding FGHigherDefault}"/>
                            <CheckBox Content="Entities" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,85,0,0" Height="16" IsChecked="{maniac:SettingBinding EntitiesDefault}"/>
                            <CheckBox Content="Animations" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,106,0,0" Height="16" IsChecked="{maniac:SettingBinding AnimationsDefault}"/>
                            <CheckBox Content="Sprites" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="21,125,0,0" Height="16" IsChecked="{maniac:SettingBinding AnimatedSpritesDefault}"/>
                            <CheckBox Content="Moving Platforms" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="21,146,0,0" Height="16" Grid.ColumnSpan="2" Width="128" IsChecked="{maniac:SettingBinding MovingPlatformsDefault}"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Other Defaults" HorizontalAlignment="Left" Height="278" VerticalAlignment="Top" Width="404" Margin="406,13,0,0" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="62*"/>
                                <ColumnDefinition Width="29*"/>
                            </Grid.ColumnDefinitions>
                            <CheckBox Content="Show Full Parallax Entity Render" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,10,0,0" Height="16" IsChecked="{maniac:SettingBinding ShowFullParallaxEntityRenderDefault}"/>
                            <CheckBox Content="Show Warp Door / Entity Arrow Paths" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,31,0,0" Height="16" IsChecked="{maniac:SettingBinding ShowEntityArrowPathsDefault}"/>
                            <CheckBox Content="Use Transparency when Editing Entities" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,52,0,0" Height="16" IsChecked="{maniac:SettingBinding EditEntitiesTransparencyDefault}"/>
                            <CheckBox Content="Enable Pixel Count Mode" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,73,0,0" Height="16" IsChecked="{maniac:SettingBinding EnablePixelModeDefault}"/>
                            <CheckBox Content="Enable Scroll Lock" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,196,0,0" Height="16" IsChecked="{maniac:SettingBinding ScrollLockEnabledDefault}"/>
                            <CheckBox Content="Show Entity Selection Boxes" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,218,0,0" Height="16" IsChecked="{maniac:SettingBinding ShowEntitySelectionBoxesDefault}"/>
                            <CheckBox Content="Show Water Level" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="232,10,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding showWaterLevelDefault}"/>
                            <CheckBox Content="Always Show Water Level" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="248,31,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding AlwaysShowWaterLevelDefault}"/>
                            <CheckBox HorizontalAlignment="Left" VerticalAlignment="Top" Margin="248,52,0,0" Height="34" Grid.ColumnSpan="2" Width="135" IsChecked="{maniac:SettingBinding SizeWaterLevelWithBoundsDefault}">
                                <TextBlock Text="Size with Bounds when Not Selected" TextWrapping="WrapWithOverflow"></TextBlock>
                            </CheckBox>
                            <CheckBox Content="Show Stats Viewer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="232,91,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding ShowStatsViewerDefault}"/>
                            <CheckBox Content="Use Large Text" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="248,112,0,0" Height="16" Grid.ColumnSpan="2" IsChecked="{maniac:SettingBinding StatsViewerLargeTextDefault}"/>
                            <GroupBox Header="Default Collision Color" HorizontalAlignment="Left" Margin="0,91,0,68" Width="144">
                                <StackPanel>
                                    <RadioButton x:Name="radioButton4" Content="Default" Margin="0,3"/>
                                    <RadioButton x:Name="radioButton3" Content="Inverted" Margin="0,3"/>
                                    <RadioButton x:Name="radioButton1" Content="Custom" Margin="0,3"/>
                                </StackPanel>
                            </GroupBox>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Faster Nudge /  Grid Defaults" HorizontalAlignment="Left" Height="131" VerticalAlignment="Top" Width="193" Margin="208,322,0,0">
                        <Grid>
                            <xctk:IntegerUpDown Name="myUpDownControl" Margin="116,26,0,54" HorizontalAlignment="Left" Width="55" Value="{maniac:SettingBinding FasterNudgeValue}"/>
                            <Label Content="Faster Nudge Amount" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="43,0,0,0" Width="128" Height="26"/>
                            <xctk:IntegerUpDown x:Name="myUpDownControl_Copy" Margin="116,76,0,4" HorizontalAlignment="Left" Width="55" Text="{maniac:SettingBinding CustomGridSizeValue}" />
                            <Label Content="Custom Grid Size" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="70,50,0,0" Width="103" Height="26"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Default Grid Size" HorizontalAlignment="Left" Margin="406,291,0,0" Width="117" Height="162" VerticalAlignment="Top" Grid.ColumnSpan="2">
                        <StackPanel>
                            <RadioButton x:Name="x16checkbox" Content="16x16" Margin="0,4,0,0" Grid.ColumnSpan="2" Checked="x16checkbox_CheckedChanged" Unchecked="x16checkbox_CheckedChanged"/>
                            <RadioButton x:Name="x128checkbox" Content="128x128" Margin="0,4,0,0" Grid.ColumnSpan="2" Checked="X128checkbox_CheckedChanged" Unchecked="X128checkbox_CheckedChanged"/>
                            <RadioButton x:Name="x256checkbox" Content="256x256" Margin="0,4,0,0" Grid.ColumnSpan="2" Checked="x256checkbox_CheckedChanged" Unchecked="x256checkbox_CheckedChanged"/>
                            <RadioButton x:Name="customGridCheckbox" Content="Custom..." Margin="0,4,0,0" Grid.ColumnSpan="2"  Checked="customGridCheckbox_CheckedChanged" Unchecked="customGridCheckbox_CheckedChanged"/>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Default Game Menu Layout" HorizontalAlignment="Left" Margin="400.333,13,0,0" Width="197" Height="259" VerticalAlignment="Top" Grid.Column="1">
                        <StackPanel x:Name="ButtonLayoutGroup">
                            <RadioButton Tag="Xbox" Content="Xbox" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PS4" Content="PS4" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="Switch" Content="Nintendo Switch (Normal)" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="Switch Joy R" Content="Nintendo Switch (Left Joycon)" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="Switch Joy L" Content="Nintendo Switch (Right Joycon)" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="Saturn Black" Content="Saturn Black" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="Saturn White" Content="Saturn White" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PC EN/JP" Content="PC EN/JP" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PC FR" Content="PC FR" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PC IT" Content="PC IT" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PC GE" Content="PC GE" Margin="0,2" Click="SetButtonLayoutDefault"/>
                            <RadioButton Tag="PC SP" Content="PC SP" Margin="0,2" Click="SetButtonLayoutDefault"/>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox Header="Default Game Menu Language" HorizontalAlignment="Left" Margin="400.333,277,0,0" Width="197" Height="176" VerticalAlignment="Top" Grid.Column="1">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Margin="5,0" x:Name="MenuLangGroup">
                                <RadioButton Content="EN" Tag="EN" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="FR" Tag="FR" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="GE" Tag="GE" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="IT" Tag="IT" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="JP" Tag="JP" Margin="0,2" Click="radioButton12_Click"/>
                            </StackPanel>
                            <StackPanel Margin="5,0" x:Name="MenuLangGroup2">
                                <RadioButton Content="KO" Tag="KO" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="SC" Tag="SC" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="SP" Tag="SP" Margin="0,2" Click="radioButton12_Click"/>
                                <RadioButton Content="TC" Tag="TC" Margin="0,2" Click="radioButton12_Click"/>
                            </StackPanel>
                        </StackPanel>




                    </GroupBox>
                    <GroupBox Header="Custom Colors" Margin="113.333,291,0,0" Height="162" VerticalAlignment="Top" HorizontalAlignment="Left" Width="282" Grid.Column="1">
                        <StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Width="20" Height="20" HorizontalAlignment="Left" Margin="2" Background="White" x:Name="CSAC" Click="comboBox4_DropDown"></Button>
                                <Label Content="Collision Solid (All) Color"></Label>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Width="20" Height="20" HorizontalAlignment="Left" Margin="2" Background="Yellow" x:Name="SSTOC" Click="comboBox6_DropDown"></Button>
                                <Label Content="Collision Solid (Top Only) Color"></Label>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Width="20" Height="20" HorizontalAlignment="Left" Margin="2" Background="Red" x:Name="CSLRDC" Click="comboBox5_DropDown"></Button>
                                <Label Content="Collision Solid (LRD) Color"></Label>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Width="20" Height="20" HorizontalAlignment="Left" Margin="2" Background="Blue" x:Name="WLC" Click="comboBox7_DropDown"></Button>
                                <Label Content="Water Level Default Color"></Label>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Width="20" Height="20" HorizontalAlignment="Left" Margin="2" Background="Black" x:Name="GDC" Click="comboBox8_DropDown"></Button>
                                <Label Content="Grid Default Color"></Label>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>

                </Grid>
            </TabItem>
            <TabItem Header="Controls" Foreground="{DynamicResource NormalText}">
                <Grid Background="{DynamicResource NormalBackground}">
                    <TabControl x:Name="ControlTab" HorizontalAlignment="Stretch" Height="453" VerticalAlignment="Stretch" Background="{DynamicResource NormalBackground}">
                        <TabItem x:Name="NewControlTab" Header="New" Foreground="{DynamicResource NormalText}">
                            <StackPanel Orientation="Horizontal" x:Name="NewControls">
                                <StackPanel Visibility="Visible">
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="New" IsEnabled="False"></Button>
                                        <Label Content="New"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="Open" IsEnabled="False"></Button>
                                        <Label Content="Open"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="OpenDataDir" IsEnabled="False"></Button>
                                        <Label Content="Open Data Directory"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="_Save" IsEnabled="False"></Button>
                                        <Label Content="Save"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="SaveAs" IsEnabled="False"></Button>
                                        <Label Content="Save As"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="UnloadScene" IsEnabled="False"/>
                                        <Label Content="Unload Scene"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Visible">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL" IsEnabled="False"></Button>
                                        <Label Content="Backup Normal"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Visible">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL" IsEnabled="False"></Button>
                                        <Label Content="Backup Stage Config"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Visible">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL" IsEnabled="False"></Button>
                                        <Label Content="Export as PNG..."></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Visible">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL" IsEnabled="False"></Button>
                                        <Label Content="Export Each Layer as PNG..."></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Visible">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL" IsEnabled="False"></Button>
                                        <Label Content="Exit"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4" Visibility="Hidden">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL"></Button>
                                        <Label Content="Title"></Label>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel  Visibility="Hidden">
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL"></Button>
                                        <Label Content="Title"></Label>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="4,4">
                                        <Button Width="100" Click="EditKeyCombo" Tag="NULL"></Button>
                                        <Label Content="Title"></Label>
                                    </StackPanel>
