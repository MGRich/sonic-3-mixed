event Create() {
    this.W = 16;
    this.H = 16;
    this.Active = true;
    this.Priority = false;

    SolidTop = true;
    Scene.AddSelfToRegistry(this, "Solid");

    Rotation = false;

    AutoAnimate = true;
}

event Render(CamX:int, CamY:int) {
    G.DrawSprite(Sprite, CurrentAnimation, Frame, X - CamX, Y - CamY, 0, IE_NOFLIP);
}
