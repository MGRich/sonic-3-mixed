object 0x6D;

event Create() {
    this.Active = true;
    this.CurrentAnimation = 13;

    this.W = 48;
    this.H = 32;

    this.X += 4;
    this.Y -= 8;
}

event Render(CamX:int, CamY:int) {
    Frame = (Scene.Frame >> 1) % 6;
    G.DrawSprite(Sprite, CurrentAnimation, Frame, X - CamX, Y - CamY, 0, IE_NOFLIP);
}
